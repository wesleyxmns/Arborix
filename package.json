{
  "name": "arborix",
  "version": "2.1.1",
  "description": "Headless React tree component - Unstyled, accessible, fully customizable tree view primitives with virtualization and drag & drop.",
  "author": "Wesley Ximenes",
  "license": "MIT",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "MIGRATION.md",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "tree",
    "treeview",
    "tree-view",
    "headless",
    "headless-ui",
    "unstyled",
    "accessible",
    "virtual",
    "virtualization",
    "drag-drop",
    "dnd",
    "arborix",
    "tree-component",
    "react-tree",
    "compound-components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wesleyxmns/Arborix.git"
  },
  "bugs": {
    "url": "https://github.com/wesleyxmns/Arborix/issues"
  },
  "homepage": "https://github.com/wesleyxmns/Arborix#readme",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "deploy": "./scripts/deploy.sh",
    "demo:dev": "pnpm --filter arborix-demo dev",
    "demo:build": "pnpm --filter arborix-demo build",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm demo:dev\" --names \"lib,demo\" --prefix-colors \"cyan,magenta\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-virtual": "^3.10.8",
    "clsx": "^2.1.1",
    "eventemitter3": "^5.0.1",
    "framer-motion": "^11.11.0",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.554.0",
    "nanoid": "^5.0.7",
    "ramda": "^0.30.1",
    "react-aria": "^3.35.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/eventemitter3": "^1.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.0",
    "@types/ramda": "^0.31.1",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "terser": "^5.44.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.4",
    "vue": "^3.5.25"
  }
}
